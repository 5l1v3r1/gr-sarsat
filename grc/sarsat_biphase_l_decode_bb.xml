<?xml version="1.0"?>
<block>
  <name>biphase_l_decode_bb</name>
  <key>sarsat_biphase_l_decode_bb</key>
  <category>[sarsat]</category>
  <import>import sarsat</import>
  <make>sarsat.biphase_l_decode_bb()</make>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
